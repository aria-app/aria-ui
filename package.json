{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "default": "./dist/index.esm.js",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build-storybook": "build-storybook",
    "build": "tsc && rollup -c",
    "ci": "yarn lint && yarn build && yarn test && yarn build-storybook",
    "clean": "rimraf build-storybook.log coverage dist storybook-static yarn-error.log",
    "lint-eslint": "eslint \"{src,stories,test}/**\" --fix",
    "lint-prettier": "prettier --write \"{src,stories,test}/**/*.{ts,tsx}\"",
    "lint": "yarn lint-eslint && yarn lint-prettier",
    "plop": "plop",
    "prepublishOnly": "yarn build",
    "semantic-release": "semantic-release",
    "size": "size-limit",
    "start": "yarn clean && start-storybook -p 6006",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --coverage --watch",
    "tsc": "tsc"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.5",
    "capsize": "^2.0.0",
    "csstype": "3.0.8",
    "focus-trap-react": "^8.7.1",
    "framer-motion": "^4.1.17",
    "lodash-es": "^4.17.21",
    "polished": "^4.1.3",
    "react-polymorphic-types": "^2.0.0",
    "react-responsive": "^8.2.0",
    "w3c-keys": "^1.0.3"
  },
  "peerDependencies": {
    "@emotion/css": ">=11",
    "@emotion/react": ">=11",
    "lodash": "^4.17.12",
    "mdi-react": "^7.5.0",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-private-property-in-object",
        {
          "loose": true
        }
      ]
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "name": "aria-ui",
  "author": "Nick Johnson",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "150 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "150 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@reach/router": "^1.3.4",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@storybook/testing-react": "^0.0.22",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/babel__core": "^7.1.15",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.172",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-is": "^17.0.2",
    "@types/react-responsive": "^8.0.3",
    "@types/semantic-release": "^17.2.1",
    "@types/storybook__addon-info": "^5.2.4",
    "@types/testing-library__jest-dom": "5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.9.2",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-testing-library": "^4.12.0",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "lodash": "^4.17.12",
    "mdi-react": "^7.5.0",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.13.0",
    "react-is": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.4.6",
    "size-limit": "^5.0.3",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aria-app/aria-ui.git"
  }
}
